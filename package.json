{
  "name": "simcore-ui",
  "version": "0.2.2",
  "description": "Component Library for SimCore apps",
  "author": "jase.smith@simcoretech.com",
  "private": true,
  "scripts": {
    "dev": "node build/dev-server.js",
    "start": "node build/dev-server.js",
    "build": "node build/build.js",
    "test": "npm run test:unit",
    "test:unit": "mocha-webpack --recursive test/unit",
    "test:unit:watch": "mocha-webpack --webpack-config build/webpack.test.conf.js --recursive -wG test/unit/utilities",
    "test:integration": "mocha-webpack -r test/integration-setup.js --webpack-config build/webpack.test.conf.js --recursive test/integration",
    "test:integration:watch": "mocha-webpack -r test/integration-setup.js --webpack-config build/webpack.test.conf.js --recursive -wG test/integration",
    "test:e2e": "cypress open",
    "test:e2e:watch": "cypress run --config watchForFileChanges=true --reporter min",
    "test:e2e:serve": "NODE_ENV=test PORT=9000 npm start",
    "lint": "eslint --ext .js,.vue components"
  },
  "dependencies": {
